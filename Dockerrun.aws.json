{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "express",
            "image": "jerrylerman/feedback-express",
            "hostname": "express",
            "essential": false,
            "portMappings": [
                {
                    "hostPort": 3000,
                    "containerPort": 3000
                }
            ],
            "links": ["express", "nginx"],
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "jerrylerman/feedback-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [{
                "hostPort": 80,
                "containerPort": 80
            }],
            "links": ["nginx", "express"],
            "memory": 128
        }
    ]
}